{
  "name": "dexscreener-js",
  "version": "1.0.0",
  "description": "TypeScript client for the DexScreener API - fetch token prices, pairs, and metrics across multiple chains",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "dexscreener",
    "crypto",
    "defi",
    "solana",
    "ethereum",
    "base",
    "token",
    "price",
    "api",
    "dex",
    "trading"
  ],
  "author": "Charles Bradley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlesScottBradley/dexscreener-js.git"
  },
  "bugs": {
    "url": "https://github.com/CharlesScottBradley/dexscreener-js/issues"
  },
  "homepage": "https://github.com/CharlesScottBradley/dexscreener-js#readme",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
